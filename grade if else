
    
    #include <stdio.h>
    
    int main(void){
    float score;
    printf ("Enter score : ");
    scanf("%f", &score);
    
    if (score>=85) {
    printf("A");
    } else if (score>=70){
    printf("B");
    } else if (score>=60){
    printf("C");
    }
    else if (score>=50){
    printf("D");
    }
    else if (score>=40){
    printf("F");
    } else
    printf("ERROR TRY AGAIN");
    
    }
