 #include<stdio.h>
 #include<stdlib.h>
 #include<time.h>

 int main() {

    int x[100] ;
    srand(time(NULL));
    for(int i=0;i<100;i++){
    x[i] = rand()% 901 +100;
    }
   for(int i=0;i<100;i++){
     printf("x%d is [%d]\n", i,x[i]);
   }
   printf("type your number = ");
   int pos;
   scanf("%d", &pos);
   printf("\nx%d is %d", pos,x[pos]);



    return (0);
 }
